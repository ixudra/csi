{
    "name": "ixudra/csi",
    "description": "Custom PHP Crash Scene Investigation (CSI) library for the Laravel 5 framework - developed by Ixudra",
    "version": "2.0.0",
    "keywords": ["Ixudra", "CSI", "Crash", "Laravel", "Exception", "Error", "Exception handling"],
    "homepage": "http://ixudra.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@ixudra.be"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "illuminate/auth": "~5.0",
        "ixudra/curl": "~6.5*"
    },

    "require-dev": {
        "phpunit/phpunit" : "~5.0",
        "mockery/mockery": "~0.9"
    },

    "autoload": {
        "psr-0": {
            "Ixudra\\Csi\\": "src/"
        }
    },

    "minimum-stability": "stable"
}
