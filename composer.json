{
    "name": "ixudra/csi",
    "description": "Custom PHP Crash Scene Investigation (CSI) library for the Laravel 4 framework - developed by Ixudra",
    "version": "1.0.6",
    "keywords": ["Ixudra", "CSI", "Crash", "Laravel", "Exception", "Error", "Exception handling"],
    "homepage": "http://ixudra.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@ixudra.be"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=4.2",
        "illuminate/auth": ">=4.2",
        "ixudra/curl": "0.1.*"
    },

    "require-dev": {
        "phpunit/phpunit" : "4.1.*",
        "mockery/mockery": "dev-master@dev"
    },

    "autoload": {
        "psr-0": {
            "Ixudra\\Csi\\": "src/"
        }
    },

    "minimum-stability": "stable"
}
